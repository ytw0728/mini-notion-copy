{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "allowJs": true,
     "jsx": "react",
    "noEmit": false,
    "strict": true,
    "moduleResolution": "node",
    "baseUrl": "src",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "typeRoots": [
      "../node_modules/@types",
      "./node_modules/@types",
      "./src/type-declares"
    ]
  },
  "include": [
    "src",
  ]
}
